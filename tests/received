<loop remotehost "" "">
<loop remoteip "" "ri">
<loop localhost "" lh>
<loop localip "" "li">
<loop helo "" "EHLO hh">

PLUGINS=add-received:accept
TESTLOCALHOST="$localhost"
TESTLOCALIP="$localip"
TESTREMOTEHOST="$remotehost"
TESTREMOTEIP="$remoteip"

$src/mailfront smtp echo 2>&1 >/dev/null <<EOF | \
	sed -n -e 's/^.* Received: //p'
$helo
MAIL FROM:<>
RCPT TO:<test@example.com>
DATA
.
EOF

<result "" "" "" "" "">
from unknown  by unknown
<result "" "" "" "" "EHLO hh">
from hh  by unknown
<result "" "" "" "li" "">
from unknown  by li ([li])
<result "" "" "" "li" "EHLO hh">
from hh  by li ([li])
<result "" "" "lh" "" "">
from unknown  by lh
<result "" "" "lh" "" "EHLO hh">
from hh  by lh
<result "" "" "lh" "li" "">
from unknown  by lh ([li])
<result "" "" "lh" "li" "EHLO hh">
from hh  by lh ([li])
<result "" "ri" "" "" "">
from ri ([ri])  by unknown
<result "" "ri" "" "" "EHLO hh">
from hh ([ri])  by unknown
<result "" "ri" "" "li" "">
from ri ([ri])  by li ([li])
<result "" "ri" "" "li" "EHLO hh">
from hh ([ri])  by li ([li])
<result "" "ri" "lh" "" "">
from ri ([ri])  by lh
<result "" "ri" "lh" "" "EHLO hh">
from hh ([ri])  by lh
<result "" "ri" "lh" "li" "">
from ri ([ri])  by lh ([li])
<result "" "ri" "lh" "li" "EHLO hh">
from hh ([ri])  by lh ([li])
<result "rh" "" "" "" "">
from rh (rh)  by unknown
<result "rh" "" "" "" "EHLO hh">
from hh (rh)  by unknown
<result "rh" "" "" "li" "">
from rh (rh)  by li ([li])
<result "rh" "" "" "li" "EHLO hh">
from hh (rh)  by li ([li])
<result "rh" "" "lh" "" "">
from rh (rh)  by lh
<result "rh" "" "lh" "" "EHLO hh">
from hh (rh)  by lh
<result "rh" "" "lh" "li" "">
from rh (rh)  by lh ([li])
<result "rh" "" "lh" "li" "EHLO hh">
from hh (rh)  by lh ([li])
<result "rh" "ri" "" "" "">
from rh (rh [ri])  by unknown
<result "rh" "ri" "" "" "EHLO hh">
from hh (rh [ri])  by unknown
<result "rh" "ri" "" "li" "">
from rh (rh [ri])  by li ([li])
<result "rh" "ri" "" "li" "EHLO hh">
from hh (rh [ri])  by li ([li])
<result "rh" "ri" "lh" "" "">
from rh (rh [ri])  by lh
<result "rh" "ri" "lh" "" "EHLO hh">
from hh (rh [ri])  by lh
<result "rh" "ri" "lh" "li" "">
from rh (rh [ri])  by lh ([li])
<result "rh" "ri" "lh" "li" "EHLO hh">
from hh (rh [ri])  by lh ([li])
