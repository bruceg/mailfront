cat >$tmp/patterns <<EOF
:header
EOF

PATTERNS=$tmp/patterns sfecho <<EOF
MAIL FROM:<>
RCPT TO:<nobody>
DATA
header

not

also
.
EOF

echo

cat >$tmp/patterns <<EOF
:not
:also
EOF

PATTERNS=$tmp/patterns sfecho <<EOF
MAIL FROM:<>
RCPT TO:<nobody>
DATA
header

not

also
.
EOF

rm -f $tmp/patterns
