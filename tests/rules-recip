cat >$tmp/rules <<EOF
k*:one@one:KK
d*:two@two:DD
z*:three@three:ZZ
zx@y:four@four:ZZZ
p*:five@five:PP
EOF

MAILRULES=$tmp/rules

sfecho <<EOF
MAIL FROM:<nobody@nobody>
RCPT TO:<one@one>
RCPT TO:<two@two>
RCPT TO:<three@three>
RCPT TO:<four@four>
RCPT TO:<five@five>
EOF

rm -f $tmp/rules

<result>
250 Sender='nobody@nobody'.^M
250 KK^M
553 DD^M
451 ZZ^M
250 Recipient='four@four'.^M
250 Recipient='five@five'.^M
