cat >$tmp/rules <<EOF
kone@one:*:KK
dtwo@two:*:DD
zthree@three:*:ZZ
zfour@four:one@one:ZZ
pfive@five:*:PP
EOF

MAILRULES=$tmp/rules

sfecho <<EOF
MAIL FROM:<one@one>
MAIL FROM:<two@two>
MAIL FROM:<three@three>
MAIL FROM:<four@four>
MAIL FROM:<five@five>
EOF

rm -f $tmp/rules

<result>
250 KK^M
553 DD^M
451 ZZ^M
250 Sender='four@four'.^M
250 Sender='five@five'.^M

